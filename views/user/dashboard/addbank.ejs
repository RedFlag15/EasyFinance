<!DOCTYPE html>
<html lang="en">

<head>
    <% include ./../../partials/dashboard/dash_head %>
</head>

<body class="fix-header fix-sidebar">
    <!-- Preloader - style you can find in spinners.css -->
    <div class="preloader">
        <svg class="circular" viewBox="25 25 50 50">
			<circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="2" stroke-miterlimit="10" /> </svg>
    </div>
    <!-- Main wrapper  -->
    <div id="main-wrapper">
        <!-- header header  -->
        <div class="header">
            <% include ./../../partials/dashboard/dash_header %>
        </div>
        <!-- End header header -->

        <!-- Left Sidebar  -->
        <div class="left-sidebar">
            <!-- Sidebar scroll-->
            <div class="scroll-sidebar">
                <!-- Sidebar navigation-->
                <% include ./../../partials/dashboard/dash_sidebar %>
            
                <!-- End Sidebar navigation -->
            </div>
            <!-- End Sidebar scroll-->
        </div>
        <!-- End Left Sidebar  -->


        <!-- Page wrapper  -->
        <div class="page-wrapper">
            <!-- Bread crumb -->
            <div class="row page-titles">
                <div class="col-md-5 align-self-center">
                    <h3 class="text-primary"><%= title %></h3> </div>
            </div>
            <!-- End Bread crumb -->
            <!-- Container fluid  -->
            <div class="container-fluid">
                <!-- Start Page Content -->
                <div class="card-body">
                    <h2 class="control-label">Search your Bank</h2>
                        <form class="form-horizontal form-material" autocomplete="off" action="/users/dashboard/add_account">
                        <div class="form-group autocomplete" style="width:300px; position:relative;">                                             
                            <input style="text-indent:30px;" id="searchbar" class="form-control form-control-line" type="text" name="search" placeholder="Type a bank...">
                            <span style="position:absolute; top:12px; bottom:12px; left:12px; font-size:20px" class="fa fa-search"></span>
                        </div>
                        </form>  
                </div>
            </div>
            <% include ./../../partials/dashboard/banklogin %>
            <!-- End Container fluid  -->
            <!-- footer -->
            
            <!-- End footer -->
        </div>
        <!-- End Page wrapper  -->
        <% include ./../../partials/dashboard/footer %>
    </div>
    <!-- End Wrapper -->
    <% include ./../../partials/dashboard/libjs %>
    <script src="/js/autocomplete.js"></script>

</body>

</html>