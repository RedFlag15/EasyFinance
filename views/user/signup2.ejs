<body class="fix-header fix-sidebar" style="background-color: #199EB8">
    <!-- Preloader - style you can find in spinners.css -->
    <div class="preloader">
        <svg class="circular" viewBox="25 25 50 50">
      <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="2" stroke-miterlimit="10" /> </svg>
    </div>
    <!-- Main wrapper  -->
    <div id="main-wrapper">

        <div class="unix-login">
            <div class="container-fluid">
                <div class="row justify-content-center">
                    <div class="col-lg-4">
                        <div class="login-content card" style="border-radius: 4%">
                            <div class="login-form">
                                <h4>Register</h4>
                                <form data-toggle="validator" role="form" method="post" action="/users/signup">
                                    <div class="form-group">
                                        <label for="inputEmail" class="control-label">Email</label>
                                        <input onkeyup="validate()" type="email" class="form-control" id="email" placeholder="Email" data-error="* Not a valid email address" required>
                                        <div id = "emailval"></div>
                                    </div>
                                    <div class="form-group">
                                        <label for="inputPassword" class="control-label">Password</label>
                                        <input onkeyup="validate()" type="password" data-minlength="8" maxlength="50"class="form-control" id="inputPassword" placeholder="Example: Abcd_1234" required>
                                        <div class="help-block" id = "passval"></div>
                                        <div class="help-block" id = "passval2"></div>
                                        <div class="help-block" id = "passval3"></div>
                                        <div class="help-block" id = "passval4"></div>
                                    </div>
                                    <div class="form-group">
                                      <label for="inputPassword" class="control-label">Password Confirm</label>
                                      <input onkeyup="validate()" type="password" class="form-control" id="inputPasswordConfirm" data-match="#inputPassword" data-match-error="Passwords don't match" placeholder="Confirm" required>
                                      <div class="help-block with-errors" id = "passconfirmation"></div>
                                    </div>                                    
                                    <button  type="submit" class="btn btn-primary btn-flat m-b-30 m-t-30">Register</button>
                                    <div class="register-link m-t-15 text-center">
                                      <p>By clicking Register, you agree to our <a target="_blank" href="/terms">Terms and Policy</a></p>
                                    </div>
                                    <div class="register-link m-t-15 text-center">
                                        <p>Already have account ? <a href="/login"> Sign in</a></p>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
    <!-- End Wrapper -->
