<div class="modal" tabindex="-1" role="dialog" id="totransactions">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title">Transactions</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close" onclick="document.getElementById('totransactions').style.display='none'">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
            <a href="/users/dashboard/own_transactions"> <button class="btn btn-primary">Own Accounts</button></a>
            <a href="/users/dashboard/other_transactions"> <button class="btn btn-primary">Registered Accounts</button></a>
        </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal" onclick="document.getElementById('totransactions').style.display='none'">Close</button>
      </div>
    </div>
  </div>
</div>