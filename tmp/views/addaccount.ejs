<!DOCTYPE html>
<html lang="en">

<head>
    <% include partials/dash_head %>
</head>

<body class="fix-header fix-sidebar">
    <!-- Preloader - style you can find in spinners.css -->
    <div class="preloader">
        <svg class="circular" viewBox="25 25 50 50">
			<circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="2" stroke-miterlimit="10" /> </svg>
    </div>
    <!-- Main wrapper  -->
    <div id="main-wrapper">
        <!-- header header  -->
        <div class="header">
            <% include partials/dash_header %>
        </div>
        <!-- End header header -->

        <!-- Left Sidebar  -->
        <div class="left-sidebar">
            <!-- Sidebar scroll-->
            <div class="scroll-sidebar">
                <!-- Sidebar navigation-->
                <% include partials/dash_sidebar %>
            
                <!-- End Sidebar navigation -->
            </div>
            <!-- End Sidebar scroll-->
        </div>
        <!-- End Left Sidebar  -->


        <!-- Page wrapper  -->
        <div class="page-wrapper">
            <!-- Bread crumb -->
            <div class="row page-titles">
                <div class="col-md-5 align-self-center">
                    <h3 class="text-primary">Add an Account</h3> </div>
            </div>
            <!-- End Bread crumb -->
            <!-- Container fluid  -->
            <div class="container-fluid">
                
                
                
                <!-- Start Page Content -->
                <div class="card-body">
                        <div class="horizontal-form-elements">
                            <form class="form-horizontal">
                                <div class="row">
                                        <div class="form-group">
                                                <label class="col-sm-2 control-label">Account Type</label>
                                                <div class="col-sm-12">
                                                    <select class="form-control" type="text" pla="Type">
                                                            <option value="" disabled selected hidden>Type</option>
                                                            <option>Credit</option>
                                                            <option>Savings</option>
                                                            <option>Current</option>
                                                        </select>
                                                </div>
                                            </div>
                                            
                                            <div class="form-group">
                                                    <label class="col-sm-2 control-label">Choose Bank </label>
                                                    <div class="col-sm-12">
                                                        <select class="form-control" type="text" pla="Type">
                                                                <option value="" disabled selected hidden>Bank</option>
                                                                <option>De base de datos</option>
                                                            </select>
                                                    </div>
                                                </div>

                                            <div class="form-group">
                                                    <label class="col-sm-2 control-label">Account Number</label>
                                                    <div class="col-sm-12">
                                                        <textarea class="form-control" rows="3" placeholder="Account Number"></textarea>
                                                    </div>
                                                </div>
                                            <div class="form-group">
                                                    <label class="col-sm-2 control-label">Good Thru</label>
                                                    <div class="col-sm-12">
                                                        <textarea class="form-control" rows="3" placeholder="01/18"></textarea>
                                                    </div>
                                                </div>
                                            
                                                <div class="form-group">
                                                        <label class="col-sm-2 control-label"></label>
                                                        <div class="col-sm-12">
                                                            <a class="btn btn-info" href="send" >Add</a>
                                                        </div>
                                                    </div>

                                </div> 
                        </form>    
                    </div>
                </div>
            </div>
            <!-- End Container fluid  -->
            <!-- footer -->
            <% include partials/footer %>
            <!-- End footer -->
        </div>
        <!-- End Page wrapper  -->
    </div>
    <!-- End Wrapper -->
    <% include partials/libjs %>

</body>

</html>